<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ChatApp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/msg"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.ChatApp.PersonOne"
            android:parentActivityName="com.example.ChatApp.MainActivity">
        </activity>
        <activity android:name="com.example.ChatApp.PersonTwo"
            android:parentActivityName="com.example.ChatApp.MainActivity">
        </activity>

        <activity android:name="com.example.ChatApp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>